<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tgy.rtls.data.mapper.type.StatusMapper">
    <select id="findByAll" resultType="com.tgy.rtls.data.entity.type.Status">
        select typeid,${name} as name
        from status
        where type=#{type}
    </select>
</mapper>